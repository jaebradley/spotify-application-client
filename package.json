{
  "name": "spotify-application-client",
  "version": "1.0.4",
  "description": "A client for the Spotify application on MacOS",
  "main": "./build/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir build",
    "compile": "babel --presets es2015,stage-0 -d build/ src/",
    "prepublish": "npm run compile",
    "testMacOS": "npm run build && mocha --compilers js:babel-core/register test/SpotifyApplicationClientIntegrationTestMacOS.js",
    "testLinux": "npm run build && mocha --compilers js:babel-core/register test/SpotifyApplicationClientIntegrationTestLinux.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/spotify-application-client.git"
  },
  "keywords": [
    "spotify"
  ],
  "author": "jaebradley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaebradley/spotify-application-client/issues"
  },
  "homepage": "https://github.com/jaebradley/spotify-application-client#readme",
  "dependencies": {
    "dbus-native": "0.2.0",
    "enumify": "^1.0.4",
    "immutable": "^3.8.1",
    "node-osascript": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-istanbul": "^0.12.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.11.16",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
